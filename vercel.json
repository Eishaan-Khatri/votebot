{
    "version": 2,
    "functions": {
        "api/main.py": {
            "runtime": "vercel-python@3.11",
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "builds": [
        {
            "src": "api/main.py",
            "use": "vercel-python@3.11"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/main.py"
        }
    ]
}